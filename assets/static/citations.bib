@Article{Einstein:1937:GW,
  author =       "A. Einstein and N. Rosen",
  title =        "On gravitational waves",
  journal =      j-J-FRANKLIN-INST,
  volume =       "223",
  number =       "1",
  pages =        "43--54",
  month =        jan,
  year =         "1937",
